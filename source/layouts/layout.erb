<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="path/to/swiper.css">

    <!-- LinkedIn -->
    <meta property='og:title' content='Lucas Porter-Bakker'/>
    <meta property='og:image' content='http://res.cloudinary.com/donw16xff/image/upload/v1533972495/Screen_Shot_2018-08-11_at_3.27.51_PM.png'/>
    <meta property='og:description' content='Full stack innovator; 5x founder and entrepreneur; experienced executive; developer and software engineer; teacher and mentor.'/>
    <meta property='og:url' content='http://res.cloudinary.com/donw16xff/image/upload/v1533972495/Screen_Shot_2018-08-11_at_3.27.51_PM.png'/>

    <!-- Facebook -->
    <meta property="og:title" content="Lucas Porter-Bakker">
    <meta property="og:description" content="Full stack innovator; 5x founder and entrepreneur; experienced executive; developer and software engineer; teacher and mentor.">
    <meta property="og:image:type" content="image/png">
    <meta property="og:url" content="http://res.cloudinary.com/donw16xff/image/upload/v1533972495/Screen_Shot_2018-08-11_at_3.27.51_PM.png">
    <meta property="og:image" content="http://res.cloudinary.com/donw16xff/image/upload/v1533972495/Screen_Shot_2018-08-11_at_3.27.51_PM.png">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title %></title>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= favicon_tag 'images/lpb.ico' %>
    <script type="text/javascript" src="application.js"></script>
  </head>
  <body>
    <script src="path/to/swiper.min.js"></script>
    <%= yield %>
  </body>
</html>
